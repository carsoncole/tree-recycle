.container
  .row
    .col-lg-12
      %h1 Logs


      %p
        = link_to admin_settings_path do
          %i.bi.bi-arrow-left
          Settings

  .row
    .col-lg-8

      = turbo_stream_from "logs"

      .pagination= pagy_bootstrap_nav(@pagy, pagy_id: 'pagination').html_safe

      #logs
        = render partial: 'admin/logs/log', collection: @logs

      .pagination= pagy_bootstrap_nav(@pagy, pagy_id: 'pagination').html_safe
